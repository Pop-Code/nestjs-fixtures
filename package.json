{
    "name": "nestjs-fixtures",
    "version": "0.0.3",
    "description": "A NestJS module to manage fixtures files",
    "author": "Alex Hermann <alex.hermann@pop-code.com>",
    "repository": "https://github.com/Pop-Code/nestjs-fixtures.git",
    "main": "build/index.js",
    "license": "MIT",
    "scripts": {
        "build": "rm -Rf build && tsc -p tsconfig.build.json",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "test:e2e:debug": "node --inspect-brk node_modules/.bin/jest --config ./test/jest-e2e.json",
        "doc": "rm -Rf docs && typedoc"
    },
    "peerDependencies": {
        "@nestjs/common": "^6.1.0",
        "nestjs-console": "^1.0.2"
    },
    "devDependencies": {
        "@nestjs/common": "6.2.4",
        "@nestjs/core": "6.2.4",
        "@types/jest": "24.0.13",
        "@types/node": "12.0.3",
        "jest": "24.7.1",
        "nestjs-console": "1.1.0",
        "reflect-metadata": "0.1.13",
        "rxjs": "6.5.2",
        "typescript": "3.5.1",
        "typedoc": "0.14.2"
    }
}
